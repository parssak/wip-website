projects.map((project, x) => (\n                // <div className=\"project\" key={\"project\" + x}>hi</div>\n                <Project project={project} x={x} key={\"project-component\" + x} />\n            ))}\n        </div>\n    )\n}\n",["92","93"],"/Users/parssa/Desktop/my-app/src/reportWebVitals.js",[],["94","95"],"/Users/parssa/Desktop/my-app/src/layout-components/NavBar.js",[],"/Users/parssa/Desktop/my-app/src/minor-components/Button.js",[],"/Users/parssa/Desktop/my-app/src/index.js",["96","97"],"/Users/parssa/Desktop/my-app/src/layout-components/Contact.js",["98"],"import React from 'react';\n\nimport Form from 'react-bootstrap/Form';\nimport Button from '../minor-components/Button';\n\n// import Hero from '../components/Hero';\n// import Content from '../components/Content';\nimport Axios from 'axios';\nimport { useSelector } from 'react-redux';\nimport Socials from '../minor-components/Socials';\n\n\nclass Contact extends React.Component {\n\n    constructor(props) {\n        super(props);\n        this.state = {\n            name: '',\n            email: '',\n            message: '',\n            disabled: false,\n            emailSent: null,\n        }\n    }\n\n\n    handleChange = (event) => {\n        const target = event.target;\n        const value = target.type === 'checkbox' ? target.checked : target.value;\n        const name = target.name;\n\n        this.setState({\n            [name]: value\n        })\n    }\n\n\n    handleSubmit = (event) => {\n        event.preventDefault();\n\n        console.log(event.target);\n\n        if (this.state.name === '' || this.state.email === '' || this.state.message === '') {\n            console.log(\"dis cap i'm outta heeeerre\");\n            return;\n        }\n\n        this.setState({\n            disabled: true\n        });\n\n        Axios.post('http://localhost:3030/api/email', this.state)\n            .then(res => {\n                if (res.data.success) {\n                    this.setState({\n                        disabled: false,\n                        emailSent: true\n                    });\n                } else {\n                    this.setState({\n                        disabled: false,\n                        emailSent: false\n                    });\n                }\n            })\n            .catch(err => {\n                console.log(err);\n\n                this.setState({\n                    disabled: false,\n                    emailSent: false\n                });\n            })\n\n    }\n\n\n    render() {\n        return (\n            <div className={`contact`} id=\"contact\">\n                <div className={`sticky-header ${this.props.theme}`}>\n                    <h3>Contact Me</h3>\n                </div>\n                <form onSubmit={this.handleSubmit}>\n                    <div className=\"fieldset\">\n                        <Form.Label htmlFor=\"full-name\">Full Name</Form.Label>\n                        <Form.Control className={`customForm ${this.props.theme}`} id=\"full-name\" name=\"name\" type=\"text\" value={this.state.name} onChange={this.handleChange} />\n                    </div>\n                        \n                    <div className=\"fieldset\">\n                        <Form.Label htmlFor=\"email\">Email</Form.Label>\n                        <Form.Control className={`customForm ${this.props.theme}`} id=\"email\" name=\"email\" type=\"email\" value={this.state.email} onChange={this.handleChange} />\n                    </div>\n\n                    <div className=\"fieldset\">\n                        <Form.Label htmlFor=\"message\">Message</Form.Label>\n                        <Form.Control className={`customForm ${this.props.theme}`}  id=\"message\" name=\"message\" as=\"textarea\" rows=\"3\" value={this.state.message} onChange={this.handleChange} />\n                    </div>\n\n                    <div className=\"flex-h-wrap\">\n                        <Button variant=\"primary\" type=\"submit\" disabled={this.state.disabled} text={\"Send message\"} helperClasses={\"rainbow-hover\"}/>\n                        <Socials />\n                    </div>\n                    \n                    {this.state.emailSent === true && <p className=\"d-inline success-msg\">Email Sent</p>}\n                    {this.state.emailSent === false && <p className=\"d-inline err-msg\">Email Not Sent</p>}\n                \n                </form>\n                \n            </div>\n        );\n    }\n\n}\n\nexport default Contact;\n","/Users/parssa/Desktop/my-app/src/reducers/themeToggler.js",[],"/Users/parssa/Desktop/my-app/src/reducers/allReducers.js",[],"/Users/parssa/Desktop/my-app/src/actions/index.js",[],"/Users/parssa/Desktop/my-app/src/minor-components/Toggle.js",[],"/Users/parssa/Desktop/my-app/src/helper-functions/FocusToggler.js",[],["99","100"],"/Users/parssa/Desktop/my-app/src/minor-components/Header.js",["101"],"import React, {useState, useEffect} from 'react'\n\n\nexport default function Header({ children, sticky = false, className, ...rest }) {\n    const [isSticky, setIsSticky] = useState(false)\n    const ref = React.createRef()\n\n    // mount \n    useEffect(() => {\n        const cachedRef = ref.current,\n            observer = new IntersectionObserver(\n                ([e]) => {\n                    setIsSticky(e.boundingClientRect.top < 0 && e.intersectionRect.top === 0);\n                },\n                { threshold: [1]})\n\n        observer.observe(cachedRef)\n\n        // unmount\n        return function () {\n            observer.unobserve(cachedRef)\n        }\n    }, [])\n\n    return (\n        <header className={className + (isSticky ? \" isSticky\" : \"\")} ref={ref} {...rest}>\n            {children}\n        </header>\n    )\n}","/Users/parssa/Desktop/my-app/src/projects.js",["102"],"/Users/parssa/Desktop/my-app/src/minor-components/Bubble.js",[],"/Users/parssa/Desktop/my-app/src/minor-components/Socials.js",[],"/Users/parssa/Desk